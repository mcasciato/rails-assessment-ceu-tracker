<% content_for :head do %>
  <%= javascript_include_tag "ceu_new/ceu_new" %>
<% end %>

<div class="container">
  <div class="row">
    <div class="col-xs-6">
      <div class="card">
        <div class="card-header display-4 childrens-4">New CEU</div>
          <div class="card-block">
            <%= render 'form' %>
          </div>
      </div>
    </div>
    <div class="col-xs-6" id="show">
      <h1 id="ceuDetails"></h1>
        <ul>
          <li id="ceuTitle"></li>
          <li id="ceuDate"></li>
          <li id="ceuLocation"></li>
          <li id="ceuDuration"></li>
          <li id="ceuCertificate"></li>
        </ul>
    </div>
  </div>
</div>
