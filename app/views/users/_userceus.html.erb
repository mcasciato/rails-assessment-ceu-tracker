<table class="table table-hover">
  <thead class="thead-default">
    <tr>
      <th><h4>Title</h4></th>
      <th><h4>Date <small>(most recent first)</small)></h4></th>
    </tr>
  </thead>
  <tbody>
      <% current_user.ceus.most_recent.uniq.each do |ceu| %>
      <tr>
        <td class="lead">
            <%= link_to ceu.title, ceu_path(ceu) %>
        </td>
        <td class="lead">
            <%= ceu.date.strftime("%m/%d/%Y") %>
        </td>
        <% end %>
      </tr>
  </tbody>
</table>
