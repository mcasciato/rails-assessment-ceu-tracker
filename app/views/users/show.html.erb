<div class="container">
  <div class="row">
    <% if current_user.admin? %>
      <h1 class="display-4 text-xs-center">Hello, <%= @user.name %>!</h1>
      <p class="lead text-xs-center">*ADMIN*</p>
        <div class="col-xs-6">
          <%= render 'user_info' %>
        </div>
        <!-- <div class="col-xs-6">
          <h3 id="ceus" class="card-header childrens-4">Your CEUs</h3>
        </div> -->
    <% else %>
      <h1 class="display-4">Hello, <%= current_user.name %>!</h1>
        <div class="col-xs-6 offset-md-3">
          <%= render 'user_info' %>
        </div>
  </div>
  <% end %>
</div>
