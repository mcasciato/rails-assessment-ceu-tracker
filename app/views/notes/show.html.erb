<div class="container">
  <div class="row">
    <div class="col-xs-6 offset-md-3">
      <div class="card">
          <h1 class="card-header display-4 childrens-8">Note Content:</h1>
          <div class="card-block">
            <p class="lead"><%= @note.content %></p>
            <%= link_to "Back", :back, class: 'btn btn-outline-info' %>
            <%= link_to "Edit Note", edit_ceu_note_path, class: 'btn btn-outline-warning' %>
            <%= link_to "Delete Note", ceu_note_path, method: :delete, class: 'btn btn-outline-danger', data: { confirm: 'Are you sure?' } %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
