<div class="card">
  <h1 class="card-header display-4 childrens-9 text-xs-center">Notes</h1>
  <div class="card-block">
    <table class="table table-hover">
      <tbody>
          <% @ceu.notes.most_recent.uniq.each do |note| %>
          <tr>
            <td class="lead">
                <% if note.content.nil? %>
                <%= link_to "Click here to Edit your First Note!", edit_ceu_note_path(@ceu.id, note) %>
                <% else %>
                <%= link_to note.content, ceu_note_path(@ceu.id, note) %>
                <% end %>
            </td>
            <% end %>
          </tr>
      </tbody>
    </table>
  </div>
</div>
