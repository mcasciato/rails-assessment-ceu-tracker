<table class="table table-hover">
  <thead class="thead childrens-9">
    <tr>
      <th>Notes</th>
    </tr>
  </thead>
  <tbody>
      <% current_user.notes.most_recent.uniq.each do |note| %>
      <tr>
        <td class="lead">
            <%= link_to note.content, edit_note_path(note) %>
        </td>
        <% end %>
      </tr>
  </tbody>
</table>
